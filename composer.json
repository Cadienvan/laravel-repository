{
    "name": "czim/laravel-repository",
    "description": "Repository for Laravel (inspired by and indebted to Bosnadev/Repositories)",
    "keywords": [
        "laravel",
        "repository",
        "repositories",
        "eloquent",
        "database",
        "criteria"
    ],
    "homepage": "https://github.com/czim/laravel-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pxlwidgets.com",
            "homepage": "http://www.pixelindustries.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0|^9.0",
        "orchestra/testbench": "^6.7.0",
        "watson/rememberable": "^5.0.1",
        "astrotomic/laravel-translatable": "^11.8",
        "czim/laravel-listify": "^1.1|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Repository\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
